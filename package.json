{
  "name": "unipage-pos",
  "version": "1.0.0",
  "author": " <>",
  "description": "Unipage Pos application",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build-all": "export CSC_IDENTITY_AUTO_DISCOVERY=false && node .electron-vue/build.js && electron-builder -mw",
    "build-mac": "export CSC_IDENTITY_AUTO_DISCOVERY=false && node .electron-vue/build.js && electron-builder -m",
    "build-windows": "node .electron-vue/build.js && electron-builder -w",
    "build:dir": "export CSC_IDENTITY_AUTO_DISCOVERY=false && node .electron-vue/build.js && electron-builder -mw --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "sign": "codesign --sign B266065C5CE4DD2E36326E68EC702FF95EE4240A --force --deep ./build/mac/unipage-pos.app",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix && electron-builder install-app-deps"
  },
  "build": {
    "asar": true,
    "productName": "unipage-pos",
    "appId": "be.wonderlus.myos-print",
    "protocols": {
      "name": "myos-deep-linking",
      "schemes": [
        "myos-print"
      ]
    },
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bluebird": "^3.5.0",
    "devtron": "^1.1.0",
    "electron-debug": "^1.4.0",
    "electron-log": "^2.2.7",
    "electron-osx-sign": "^0.4.7",
    "element-ui": "^2.0.0-alpha.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "raven": "^2.3.0",
    "raven-js": "^3.20.1",
    "route-parser": "^0.0.5",
    "sass-loader": "^6.0.6",
    "store": "^2.0.12",
    "vue": "2.5.2",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "electron": "^1.7.9",
    "electron-builder": "19.27.0",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.13.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "multispinner": "^0.2.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "2.5.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.18.0"
  }
}
